  object dtsEscolha: TDataSource
    DataSet = qryEscolha
    Left = 42
    Top = 194
  end
  object qryEscolha: TQuery
    DatabaseName = 'NL'
    SessionName = 'Session1_1'
    SQL.Strings = (
      'select distinct usu.cod_usuario as cod, usu.cod_grupo as grupo'
      
        '                        from ge_usuarios usu,(Select g.cod_usuar' +
        'io   '
      
        '                        FROM v$session s, ge_sessoes g WHERE g.c' +
        'od_sessao = s.audsid  '
      '                         and UPPER(s.OSUSER) = UPPER(383197)) a'
      '                        where usu.cod_usuario = a.cod_usuario')
    Left = 9
    Top = 195
  end
